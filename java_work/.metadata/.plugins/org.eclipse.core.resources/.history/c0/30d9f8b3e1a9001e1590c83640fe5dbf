package programmer;

public class sheep {
	public static void main(String[] args) {
		int n = 10;
		int k = 3;
		int answer = 0;
         
		 n = 12000 * n;
		 System.out.println("n"+n);
		 k = (k - n/10) * 2000;
		 System.out.println("k"+k);
        	 
	     answer = n + k;
	     System.out.println(answer);
	     
	}
}
