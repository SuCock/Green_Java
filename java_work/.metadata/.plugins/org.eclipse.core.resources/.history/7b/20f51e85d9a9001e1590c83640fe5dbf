package programmer;

public class sheep {
	public static void main(String[] args) {
		int k = 0;
		int n = 0;
		int answer = 0;
         
         for(int i = 0; i<n; i++){
             n = i + 12000;
         }
         for(int j = 0; j<k; j++){
             if(n%10 == 0){
                 k = j - 2000;
         }else{
                 k = j + 2000;
             }
     }
     answer = n + k;
     System.out.println(answer);
	}
}
