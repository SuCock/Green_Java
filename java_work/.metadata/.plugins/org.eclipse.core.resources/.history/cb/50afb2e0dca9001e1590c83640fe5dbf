package programmer;

public class sheep {
	public static void main(String[] args) {
		int n = 64;
		int k = 6;
		int k2 = 0;
		int answer = 0;
         
         if(n%10 == 0) {
        	 n = 12000 * n;
        	 k = 2000 * (k-1);
        	 k2 = 2000 * (n%10);

         }else{
        	 n = 12000 * n;
        	 k = 2000 * k;
         }
     answer = n + k + k2;
     System.out.println(answer);
     
	}
}
