package programmer;

public class sheep {
	public static void main(String[] args) {
		int n = 10;
		int k = 3;
		int answer = 0;
         
         if(n%10 == 0) {
        	 n = 12000 * n;
        	 k = 2000 * k - (2000 * n);
         }else {
        	 n = 12000 * n;
        	 k = 2000 * k;
         }
     answer = n + k;
     System.out.println(answer);
     
	}
}
