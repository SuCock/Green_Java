package programmer;

public class sheep {
	public static void main(String[] args) {
		int n = 64;
		int k = 6;
		int answer = 0;
         
         if(n%10 == 0) {
        	 n = 12000 * n;
        	 k = 2000 * (k-1);
        	 System.out.println(n+k);
         }else{
        	 n = 12000 * n;
        	 k = 2000 * k;
        	 System.out.println(n+k);
         }
     //answer = n + k;
     //System.out.println(answer);
     
	}
}
