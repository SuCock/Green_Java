package programmer;

public class letter {
	public static void main(String[] args) {
		String my_string = "";
		String letter = "";
		String answer = "";
		
		// for문을 돌려서 my_string을 확인
		// 특정문자 letter를 조건문을 넣어서 제거
		
		for(int i = 0; i<my_string.length(); i++) {
			char my_char = my_string.charAt(i);
			System.out.println(my_char);
		}
	}
}
