package ex01;

public class Ex02 {
	public static void main(String[] args) {
		int a = 10;
		System.out.println("a = " + a);
		
		int b = 20;
		System.out.println("a + b = " + a+b); // 1020 문자열 연산(나열)
		System.out.println("a + b = " + (a+b)); // 30 
		
		String c = "a";
		String d = "b";
		System.out.println(c+d);
	}
}
