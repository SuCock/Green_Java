package programmer;

public class sheep {
	public static void main(String[] args) {
		int n = 10;
		int k = 3;
		int answer = 0;
         
         for(int i = 0; i<n; i++){
             n += 12000;
         }
         for(int j = 0; j<k; j++){
             if(n%10 == 0){
                 k -= 2000;
         }else{
                 k += 2000;
             }
     }
     answer = n + k;
     System.out.println(answer);
	}
}
